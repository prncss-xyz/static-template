{
	"name": "laideurs",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "waku dev",
		"dev:tests": "vitest run --watch",
		"build": "waku build",
		"start": "waku start",
		"knip": "knip --cache",
		"check:types": "tsc --noEmit",
		"check:tests": "vitest run --coverage --passWithNoTests",
		"check": "pnpm run '/^check:.*/'",
		"fix:prettier": "prettier --write --cache --cache-strategy metadata .",
		"fix-ci:prettier": "prettier --check --cache --cache-strategy metadata .",
		"fix-ci:eslint": "eslint --cache --max-warnings 0 --no-warn-ignored",
		"fix:eslint": "eslint --cache --max-warnings 0 --no-warn-ignored --fix",
		"fix": "pnpm run '/^fix:.*/'",
		"fix-ci": "pnpm run '/^fix-ci:.*/'",
		"verify": "pnpm run check && pnpm run fix && pnpm run build",
		"verify-ci": "pnpm run check && pnpm run fix-ci"
	},
	"dependencies": {
		"@stylexjs/stylex": "^0.17.4",
		"react": "~19.2.3",
		"react-dom": "~19.2.3",
		"react-server-dom-webpack": "~19.2.3",
		"waku": "1.0.0-alpha.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.3.0",
		"@commitlint/config-conventional": "^20.3.0",
		"@cspell/eslint-plugin": "^9.4.0",
		"@eslint/js": "^9.39.2",
		"@stylexjs/eslint-plugin": "^0.17.4",
		"@stylexjs/unplugin": "^0.17.4",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.16",
		"babel-plugin-react-compiler": "^1.0.0",
		"eslint": "^9.39.2",
		"eslint-plugin-only-warn": "^1.1.0",
		"eslint-plugin-perfectionist": "^5.2.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"globals": "^17.0.0",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"knip": "^5.79.0",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vitest": "^4.0.16"
	}
}
